                                                             data-total-hours-forecasted="<%= @total_hours_forecasted.inspect %>"
                                                             data-weekly-goal-hours="<%= @weekly_goal_hours.inspect %>"
                                                             data-weekly-billed="<%= @weekly_billed.inspect %>"></canvas>


